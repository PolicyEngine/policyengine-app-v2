{
  "version": 2,
  "buildCommand": "cd app && npm run build",
  "outputDirectory": "app/dist",
  "framework": null,
  "functions": {
    "app/api/*.ts": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/app/api/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
